<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.solidfrost.android.customheaderpacks">

    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity  android:label="Death Note" android:name="com.solidfrost.android.customheaderpacks.headers.deathnote">
            <intent-filter>
                <action android:name="org.omnirom.DaylightHeaderPack1"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>

        </activity>
        <activity android:label="Sonic OC with accent" android:name="com.solidfrost.android.customheaderpacks.headers.sonic">
        <intent-filter>
            <action android:name="org.omnirom.DaylightHeaderPack1"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
        </activity>
        <activity android:label="Detective Conan" android:name="com.solidfrost.android.customheaderpacks.headers.conan">
            <intent-filter>
                <action android:name="org.omnirom.DaylightHeaderPack1"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>

        </activity>
        <activity android:label="Anime Places" android:name="com.solidfrost.android.customheaderpacks.headers.anime">
            <intent-filter>
                <action android:name="org.omnirom.DaylightHeaderPack1"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>

        </activity>
    </application>

</manifest>